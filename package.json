{
  "name": "dom-to-react",
  "version": "0.0.4",
  "description": "A versatile replacement for 'dangerouslySetInnerHTML'. Let's you build react-components deep within a regular HTML-Dom",
  "keywords": [
    "dangerouslySetInnerHTML",
    "react",
    "component"
  ],
  "main": "dist/dom-to-react.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:dist": "cross-env NODE_ENV=production webpack",
    "build:dist:min": "cross-env NODE_ENV=production webpack -p",
    "build": "npm run clean && npm run build:dist && npm run build:dist:min",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=development webpack-dev-server"
  },
  "readme": "README.md",
  "author": "Andreas Hahn <andreas.hahn@diva-e.com>",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/diva-e/dom-to-react.git"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "cross-env": "^2.0.1",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-markdown": "^2.4.2",
    "react-router": "^4.0.0-alpha.3",
    "rimraf": "^2.5.4",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.4"
  }
}
